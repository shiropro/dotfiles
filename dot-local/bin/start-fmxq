#!/bin/sh
# -----------------------------------------------------------------------------
# - 无退出快捷键模式
# -----------------------------------------------------------------------------
# 避免在 ranger 启动的 shell 内创建新的 ranger
[ -n "$RANGER_LEVEL" ] && exit 1
# -----------------------------------------------------------------------------
_unset_quit="chain unmap q;unmap Q;unmap <F10>"
exec start-fm "--cmd=$_unset_quit" "$@"
